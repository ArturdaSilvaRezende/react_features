{
    "compilerOptions": {
      "baseUrl": ".",
      "module": "es2020",
      "moduleResolution": "node",
      "target": "es2020",
      "esModuleInterop": true,
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx",
     
      "paths": {
        "~/*": ["./src/*"]
      }
    },
    "include": ["src"]
  }